<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ShowHidePasswordEditText">
        <attr name="drawable_hide" format="integer" />
        <attr name="drawable_show" format="integer" />
        <attr name="tint_color" format="color" />
        <attr name="additionalTouchTargetSize" format="dimension" />
    </declare-styleable>
    <declare-styleable name="ClearableEditText">
        <attr name="clearIconDrawable" format="color|reference" />
        <attr name="clearIconDrawWhenFocused" format="boolean" />
    </declare-styleable>
    <declare-styleable name="PinEntryEditText">
        <attr name="pinAnimationType" format="enum">
            <enum name="popIn" value="0" />
            <enum name="fromBottom" value="1" />
            <enum name="none" value="-1" />
        </attr>
        <attr name="pinCharacterMask" format="string" />
        <attr name="pinRepeatedHint" format="string" />
        <attr name="pinLineStroke" format="dimension" />
        <attr name="pinLineStrokeSelected" format="dimension" />
        <attr name="pinCharacterSpacing" format="dimension" />
        <attr name="pinTextBottomPadding" format="dimension" />
        <attr name="pinLineColors" format="color" />
        <attr name="pinBackgroundDrawable" format="reference" />
        <attr name="pinBackgroundIsSquare" format="boolean" />
    </declare-styleable>
    <declare-styleable name="RPView">
        <attr name="Font" format="string" />
        <attr name="starPin" format="boolean" />
    </declare-styleable>
    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_circle_background_color" format="color" />
    </declare-styleable>
    <declare-styleable name="BbynSeekBar">
        <attr name="progressTextColor" format="integer"/>
        <attr name="maxValue" format="integer"/>
        <attr name="thumb" format="integer"/>
        <attr name="progressDrawable" format="integer"/>
        <attr name="startValue" format="integer"/>/>
    </declare-styleable>
</resources>